; SOA Records
$TTL 3600
$ORIGIN jory.dev.
@ 3600 IN SOA jory.dev. pompom.internal. (
    0000000001         ; serial number
    7200               ; refresh period
    3600               ; retry period
    1209600            ; expire time
    3600               ; minimum ttl
)

; NS Records
@                          IN  NS     pompom.internal.

; LAN
unifi                      IN  A      192.168.1.1

; Servers
voyager                    In  A      10.69.1.100

; Containers
bws-cache                  IN  A      192.168.1.4
traefik                    IN  A      192.168.1.4
gatus                      IN  A      192.168.1.4

; CNAME records
minio                      IN  CNAME  voyager.jory.dev.
nas                        IN  CNAME  voyager.jory.dev.
portainer                  IN  CNAME  voyager.jory.dev.
s3                         IN  CNAME  voyager.jory.dev.
